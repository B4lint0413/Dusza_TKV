<Window x:Class="DuszaWpfApp.EndGameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:DuszaWpfApp"
        mc:Ignorable="d"
        Title="End Game" Height="450" Width="800" MinWidth="360">
    <ScrollViewer>
        <StackPanel Width="320" Margin="10">
            <TextBlock HorizontalAlignment="Center" Name="Title" FontSize="30" FontWeight="Bold" Margin="10">End </TextBlock>
            <StackPanel Name="Container" Margin="10">
            
            </StackPanel>
            <Button Margin="20" Height="25" Click="CommitResults" Content="Commit Results"/>
            <Button Margin="20" Height="25" Click="ToNavigationWindow" Content="Back to Navigation"/>
        </StackPanel>
    </ScrollViewer>
</Window>
