<Window x:Class="DuszaWpfApp.LeaderboardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DuszaWpfApp"
        mc:Ignorable="d"
        Title="LeaderboardWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock HorizontalAlignment="Center" FontSize="30" FontWeight="Bold" Margin="10">Leaderboard</TextBlock>
            <ListBox HorizontalAlignment="Center" FontSize="30" FontWeight="Bold" Margin="10" Name="leaderboard">

            </ListBox>
            <Button MaxWidth="160" Margin="10" Height="25" Click="ToNavigationWindow" Content="Back to Navigation"/>
        </StackPanel>
    </Grid>
</Window>
